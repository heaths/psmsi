        <Name>ReplacedPatchCodes</Name>
        <GetScriptBlock>
          $length = $this.RevisionNumber.Length
          if ($length -gt 38)
          {
          $this.RevisionNumber.Substring(38, $length - 38) -split "(})"
          }
        </GetScriptBlock>
      </ScriptProperty>
