        <Name>MinimumVersion</Name>
        <GetScriptBlock>
          switch ($this.WordCount)
          {
          2 { [Version] 1.2 }
          3 { [Version] 2.0 }
          4 { [Version] 3.0 }
          5 { [Version] 3.1 }
          default { [Version] 1.0 }
          }
        </GetScriptBlock>
